{
  "name": "ComuniDADOSReact",
  "version": "2.0.1",
  "description": "This is the comunidados front-end application for viewing shapes and survey data. Built with React.",
  "keywords": [
    "react",
    "leaflet",
    "material-ui"
  ],
  "main": "src/index.js",
  "homepage": "https://comunidados-react.s3.amazonaws.com/",
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "4.2.1",
    "Leaflet.vector-markers": "0.0.6",
    "axios": "0.19.0",
    "env-cmd": "^10.0.1",
    "leaflet": "1.5.1",
    "lodash": "4.17.15",
    "nice-color-palettes": "^3.0.0",
    "react": "16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-autosuggest-dropdown-menu": "^2.0.1",
    "react-dom": "16.8.6",
    "react-input-enhancements": "^0.7.5",
    "react-leaflet": "^2.5.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.8",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "shortid": "2.2.14"
  },
  "devDependencies": {
    "bestzip": "^2.1.5",
    "typescript": "3.3.3"
  },
  "scripts": {
    "deploy": "npm run build && npm run zip && npm run s3_upload",
    "build": "react-scripts build",
    "dev": "env-cmd -f .env.development react-scripts start",
    "dev:staging": "env-cmd -f .env.dev-staging react-scripts build",
    "prod:dev": "env-cmd -f .env.staging react-scripts start",
    "prod:staging": "env-cmd -f .env.staging react-scripts build",
    "s3_upload": "aws s3 cp ./bundle.zip s3://comunidados-react-v3/bundle.zip --acl public-read",
    "doitall": "npm run build && npm run zip && npm run s3_upload && npm run git_update",
    "eject": "react-scripts eject",
    "git_test": "python ./config/git_test.py",
    "git_update": "python ./config/git_update.py",
    "zip": "bestzip bundle.zip build/*"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
