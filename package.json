{
  "name": "ComuniDADOSReact",
  "version": "1.0.0",
  "description": "This is the comunidados front-end application for viewing shapes and survey data. Built with React.",
  "keywords": ["react","leaflet","material-ui"],
  "main": "src/index.js",
  "homepage": "https://comunidados-react.s3.amazonaws.com/",
  "dependencies": {
    "@material-ui/core": "4.4.0",
    "@material-ui/icons": "4.2.1",
    "axios": "0.19.0",
    "leaflet": "1.5.1",
    "lodash": "4.17.15",
    "nice-color-palettes": "^3.0.0",
    "react": "16.8.6",
    "react-autosuggest": "^9.4.3",
    "react-autosuggest-dropdown-menu": "^2.0.1",
    "react-dom": "16.8.6",
    "react-input-enhancements": "^0.7.5",
    "react-leaflet": "^2.4.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1",
    "react-select": "^3.0.4",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "shortid": "2.2.14"
  },
  "devDependencies": {
    "typescript": "3.3.3"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
		"deploy": "aws s3 sync build/ s3://comunidados-react",
    "test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"git_update": "python ./config/git_update.py",
		"do_it_all": "npm run build && npm run deploy && git add . && git commit -m \"tiny labelling updates\" && git push origin master"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
